<svg width="256" height="256" viewBox="0 0 256 256" xmlns="http://www.w3.org/2000/svg">
  <!-- Background circle -->
  <circle cx="128" cy="128" r="120" fill="#2563eb" stroke="#1e40af" stroke-width="4"/>
  
  <!-- Bicycle wheel outer ring -->
  <circle cx="128" cy="128" r="85" fill="none" stroke="#ffffff" stroke-width="6"/>
  
  <!-- Spokes -->
  <g stroke="#ffffff" stroke-width="3" stroke-linecap="round">
    <!-- Vertical and horizontal spokes -->
    <line x1="128" y1="43" x2="128" y2="213"/>
    <line x1="43" y1="128" x2="213" y2="128"/>
    
    <!-- Diagonal spokes -->
    <line x1="67.4" y1="67.4" x2="188.6" y2="188.6"/>
    <line x1="188.6" y1="67.4" x2="67.4" y2="188.6"/>
    
    <!-- Additional spokes for detail -->
    <line x1="92.8" y1="49.4" x2="163.2" y2="206.6"/>
    <line x1="206.6" y1="92.8" x2="49.4" y2="163.2"/>
    <line x1="163.2" y1="49.4" x2="92.8" y2="206.6"/>
    <line x1="49.4" y1="92.8" x2="206.6" y2="163.2"/>
  </g>
  
  <!-- Center hub -->
  <circle cx="128" cy="128" r="15" fill="#ffffff"/>
  <circle cx="128" cy="128" r="8" fill="#2563eb"/>
  
  <!-- Company initials in center -->
  <text x="128" y="135" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#ffffff">RCM</text>
  
  <!-- Tire rim effect -->
  <circle cx="128" cy="128" r="100" fill="none" stroke="#e5e7eb" stroke-width="2"/>
  <circle cx="128" cy="128" r="70" fill="none" stroke="#e5e7eb" stroke-width="1"/>
</svg>